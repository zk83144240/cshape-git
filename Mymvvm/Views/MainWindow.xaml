<Window x:Class="Mymvvm.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mymvvm.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Height="Auto" Width="Auto" Background="Beige">
        <TextBlock Text="{Binding Value}" Height="50" Width="500" Background="BlueViolet" Margin="10"/>
    </StackPanel>
</Window>
